{
  "name": "gh-workflow-expr-highlight",
  "displayName": "GitHub Workflow Expression ${{}} Highlighter",
  "description": "An extension which highlights ${{ }} pattern in YAML files",
  "publisher": "adamraichu",
  "icon": "logo.png",
  "authors": "AdamRaichu",
  "author": {
    "name": "AdamRaichu"
  },
  "categories": [],
  "repository": {
    "url": "https://github.com/AdamRaichu/vscode-webext-template"
  },
  "main": "./extension.js",
  "browser": "./extension.js",
  "version": "1.0.0",
  "activationEvents": [],
  "engines": {
    "vscode": "^1.63.0"
  },
  "contributes": {
    "grammars": [
      {
        "scopeName": "source.expr-highlight.injection",
        "path": "./syntaxes/injection.tmLanguage.json",
        "injectTo": [
          "source.yaml"
        ]
      }
    ]
  }
}